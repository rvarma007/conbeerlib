<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.suvikas.conware">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.suvikas.conware.Activities.DetectContainer" />
        <activity android:name="com.suvikas.conware.Activities.AttackAndOTP" />
        <activity
            android:name="com.suvikas.conware.Activities.CommandExecActivity"
            android:label="@string/title_activity_command_exec"
            android:parentActivityName="com.suvikas.conware.MainActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="adjustPan">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.suvikas.conware.MainActivity" />
        </activity>
        <activity
            android:name="com.suvikas.conware.Activities.RunningAppsListActivity"
            android:label="@string/title_running_apps_list"
            android:parentActivityName="com.suvikas.conware.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.suvikas.conware.MainActivity" />
        </activity>
        <activity
            android:name="com.suvikas.conware.Activities.KeystoreActivity"
            android:label="@string/title_activity_key_store_key_list"
            android:parentActivityName="com.suvikas.conware.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.suvikas.conware.MainActivity" />
        </activity>
        <activity
            android:name="com.suvikas.conware.Activities.AppListActivity"
            android:label="@string/title_activity_app_list"
            android:parentActivityName="com.suvikas.conware.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.suvikas.conware.MainActivity" />
        </activity>
        <activity
            android:name="com.suvikas.conware.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>